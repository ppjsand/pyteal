  <analyze_events id_regx=".*"/>
</gear_control> -->
<events>
   <event id="Example5" comp="PS" name="REFNAME"/>
   <event id="Example2"/>
   <event id="Example1" pool_extension_time="3" min_time_in_pool="5"/>   
</events>
