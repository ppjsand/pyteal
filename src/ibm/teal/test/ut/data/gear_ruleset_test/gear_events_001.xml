<gear_ruleset>
<pool_control>
  <initial_duration default="300"/>
</pool_control>
<gear_control>
  <default_event_comp value="TC"/>
  <!-- <analyze_alerts id_regx="  Z  ,   B,Q"/>-->
  <analyze_events id_regx=".*"/>
</gear_control> -->
<events>
   <event id="Example5" comp="PS" name="REFNAME"/>
   <event id="Example2"/>
   <event id="Example1" pool_extension_time="3" min_time_in_pool="5" comp="TC2"/>   
