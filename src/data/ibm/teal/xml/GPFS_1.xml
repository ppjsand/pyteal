        <column name="meta_exposed" type="VARCHAR" size="256"/>
        <column name="pool_name" type="VARCHAR" size="256"/>
        <column name="pool_status" type="VARCHAR" size="256"/>
        <column name="pool_usage" type="INTEGER"/>
    </table>
    
    <table name="disk" id="2">
        <column name="severity" type="VARCHAR" size="256"/>
        <column name="fs_name" type="VARCHAR" size="256"/>
