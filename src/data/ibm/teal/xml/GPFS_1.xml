<?xml version="1.0" encoding="UTF-8"?>

<tables xmlns="http://www.ibm.com/teal" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.ibm.com/teal ../../../../xml/extension.xsd ">

    <table name="filesystem" id="1">
        <column name="severity" type="VARCHAR" size="256"/>
        <column name="node_ip" type="VARCHAR" size="256"/>
        <column name="sgmgr_ip" type="VARCHAR" size="256"/>
        <column name="user_unba" type="VARCHAR" size="256"/>
